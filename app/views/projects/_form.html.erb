

<%= form_for @project do |f| %>
  <label>Title</label>
  <br>
  <%= f.text_field :title %>
  <br>
  <label>Description</label>
  <br>
  <%= f.text_area :description, rows: 5 %>
  <br>
  <label>Due Date</label>
  <br>
  <%= f.datetime_select :due_date %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>